    "eslint": "^7.32.0",
    "eslint-config-screwdriver": "^5.0.1",
    "mocha": "^8.4.0",
    "mocha-multi-reporters": "^1.5.1",
    "mocha-sonarqube-reporter": "^1.0.2",
    "mockery": "^2.0.0",
    "nyc": "^15.0.0",
    "sinon": "^9.0.0"
  },
  "dependencies": {
    "@hapi/hoek": "^9.2.1",
    "circuit-fuses": "^4.1.2",
    "joi": "^17.6.0",
    "screwdriver-data-schema": "^21.22.2",
    "screwdriver-logger": "^1.1.0",

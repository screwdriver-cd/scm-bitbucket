  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/screwdriver-cd/scm-bitbucket.git"
  },
  "homepage": "https://github.com/screwdriver-cd/scm-bitbucket",
  "bugs": "https://github.com/screwdriver-cd/screwdriver/issues",
  "keywords": [
    "screwdriver",
    "yahoo"
  ],
